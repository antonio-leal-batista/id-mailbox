{
  "$schema": "http://json-schema.org/schema#",
  "$id": "#didDoc",
  "type": "object",
  "properties": {
    "@context": {
      "type": "string",
      "example": "https://w3id.org/did/v1",
      "enum": ["https://w3id.org/did/v1"]
    },
    "id": {
      "type": "string",
      "example": "did:elem:EHyX7W2yNXL3-tJ2ArauoUMuPmp7afOx8Zi-fc2oXww"
    },
    "publicKey": {
      "type": "array",
      "example": [
        {
          "id": "#key1",
          "type": "Secp256k1VerificationKey2018",
          "publicKeyHex": "037cb462083130f5c42ff2b15bc9eaa397386ae86d96045b89a5eec9ccccb2cbef"
        }
      ]
    }
  },
  "required": ["@context", "id", "publicKey"]
}
